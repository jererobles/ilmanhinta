{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "RAILPACK"
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "ALWAYS",
    "sleepApplication": false,
    "startCommand": "bash scripts/start-etl.sh"
  },
  "variables": {
    "SERVICE_TYPE": "etl",
    "DAGSTER_HOME": "/app/dagster_home",
    "PREDICTION_MODEL_TYPE": "lightgbm",
    "LOG_LEVEL": "INFO"
  }
}
